package Sorting;

public class BubbleSort {
	
	public static void main(String[] args) {
		int[] arr = {54,34,2,42,4,21,1};
	}
	
	public static int[] sort(int[] arr) {
		
		for(int i = 0; i < arr.length - 1; i++) {
			for(int j = 1; j < arr.length; j++) {
				if(arr[j] < arr[i]) {
					int temp = arr[i];
					arr[i] = arr[j];
					arr[j] = temp;
				}
			}
		}
		
		return arr;
		
	}
}
