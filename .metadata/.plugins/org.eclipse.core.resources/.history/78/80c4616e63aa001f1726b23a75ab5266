package Sorting;

public class MergeSort {
	
	public static void main(String[] args) {
		
		int[] array = new int[6];
		
		for(int i = 0; i < 6; i++) {
			array[i] = i+1;
		}
		
		for(int j = 0; j < array.length; j++) {
			System.out.print(array[j]);
		}
	}
	
	public static void sort(int[] array) {
		
	}
} 
